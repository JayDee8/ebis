@model ebis.Models.produkcni_listy

@{
    ViewBag.Title = "Detaily produkčních listů";
}

<h2>@ViewBag.Title</h2>

<fieldset>
    <legend>produkcni_listy</legend>

    <div class="form-group row">
        <label class="col-md-2 control-label">Jméno aktivity</label>
        <div class="col-md-10">@Html.DisplayFor(model => model.jmeno_aktivity)</div>
    </div>
    <div class="form-group row">
        <label class="col-md-2 control-label">Typ aktivity</label>
        <div class="col-md-10">@Html.DisplayFor(model => model.typ)</div>
    </div>

    
</fieldset>
<p>
    @Html.ActionLink("Upravit produkční listy", "Edit", new { id=Model.pk_id }) |
    @Html.ActionLink("Zpět na seznam", "Index")
</p>
